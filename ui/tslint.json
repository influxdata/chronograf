{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "jsRules": {
    "object-literal-sort-keys": false,
    "ordered-import": false
  },
  "rules": {
    "object-literal-sort-keys": false,
    "ordered-import": false,
    "interface-name": [true, "never-prefix"],
    "no-console": [true, "log", "warn"],
    "no-empty": false,
    "prettier": [
      true,
      {
        "singleQuote": true,
        "semi": false,
        "trailingComma": "es5",
        "bracketSpacing": false
      }
    ]
  },
  "rulesDirectory": []
}
