{
  "compilerOptions": {
    "types": [
      "node",
      "mocha",
      "chai",
      "lodash"
    ],
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": [
      "es6",
      "es2017",
      "dom"
    ],
    "skipLibCheck": true,
    "isolatedModules": false,
    "jsx": "react",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noImplicitAny": false,
    "noImplicitUseStrict": false,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "removeComments": true,
    "importHelpers": true,
    "noLib": false,
    "noEmitHelpers": true,
    "noEmit": true,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "checkJs": false,
    "baseUrl": "./",
    "sourceMap": true,
    "rootDirs": [
      "src",
      "spec"
    ],
    "paths": {
      "*": [
        "*"
      ],
      "src/*": [
        "src/*"
      ],
      "admin/*": [
        "src/admin/*"
      ],
      "alerts/*": [
        "src/alerts/*"
      ],
      "auth/*": [
        "src/auth/*"
      ],
      "dashboards/*": [
        "src/dashboards/*"
      ],
      "data_explorer/*": [
        "src/data_explorer/*"
      ],
      "external/*": [
        "src/external/*"
      ],
      "hosts/*": [
        "src/hosts/*"
      ],
      "influxql/*": [
        "src/influxql/*"
      ],
      "kapacitor/*": [
        "src/kapacitor/*"
      ],
      "normalizers/*": [
        "src/normalizers/*"
      ],
      "shared/*": [
        "src/shared/*"
      ],
      "side_nav/*": [
        "src/side_nav/*"
      ],
      "sources/*": [
        "src/sources/*"
      ],
      "status/*": [
        "src/status/*"
      ],
      "store/*": [
        "src/store/*"
      ],
      "style/*": [
        "src/style/*"
      ],
      "utils/*": [
        "src/utils/*"
      ]
    }
  },
  "exclude": [
    "./build/*",
    "./node_modules/*"
  ]
}