{
    "user": {
        "name": "Smiley",
        "role": ["reader", "viewer"],
        "oauthProvider": "oauth-mock",
        "oauth-schema": "oauth",
        "orgs": ["Default"]
    },
    "organizations": [
        {
            "name": "SmallOrg",
            "defaultRole": "reader",
            "mapping": {
                "scheme": "oauth2",
                "provider": "oauth-mock",
                "providerOrg": "default"
            }
        },
        {
            "name": "BigOrg",
            "defaultRole": "member",
            "mapping": {
                "scheme": "oauth2",
                "provider": "oauth-mock-2",
                "providerOrg": "default-2"
            }
        }
    ]
}