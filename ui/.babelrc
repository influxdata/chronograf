{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-class-properties",
    "babel-plugin-lodash",
    "istanbul",
    [
      "babel-plugin-module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "src": "./src",
          "shared": "./src/shared",
          "style": "./src/style",
          "utils": "./src/utils"
        }
      }
    ]
  ]
}